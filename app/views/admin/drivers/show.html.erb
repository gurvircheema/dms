<h4 class='mt-3 pb-3'>Driver Information <small class='text-muted'><%= @driver.name %></small></h4>
<div class='row'>
  <div class='col-md-6'>
    <table class='table table-sm table-bordered'>
      <tr>
        <td>Name</td>
        <td><%= @driver.name %></td>
      </tr>
      <tr>
        <td>Address</td>
        <td><%= @driver.address %></td>
      </tr>
      <tr>
        <td>City</td>
        <td><%= @driver.city %></td>
      </tr>
      <tr>
        <td>State</td>
        <td><%= @driver.state %></td>
      </tr>
      <tr>
        <td>Country</td>
        <td><%= @driver.country %></td>
      </tr>
      <tr>
        <td>Zip</td>
        <td><%= @driver.zip %></td>
      </tr>
      <tr>
        <td>Phone</td>
        <td><%= @driver.phone %></td>
      </tr>
      <tr>
        <td>Cell</td>
        <td><%= @driver.cell %></td>
      </tr>
      <tr>
        <td>Email</td>
        <td><%= @driver.email %></td>
      </tr>
      <tr>
        <td>Status Active</td>
        <td><%= @driver.active %></td>
      </tr>
      <tr>
        <td>Date of Birth</td>
        <td><%= @driver.date_of_birth %></td>
      </tr>
      <tr>
        <td>Ability to work</td>
        <td><%= @driver.ability_to_work %></td>
      </tr>
      <tr>
        <td>Driver Type</td>
        <td><%= @driver.driver_type %></td>
      </tr>
    </table>
  </div>
  <div class='col-md-6'>
  </div>
</div>
<%= link_to 'Back', [:admin, :drivers], class: 'btn btn-secondary' %>
<%= link_to 'Edit', [:edit, :admin, @driver], class: 'btn btn-info' %>
